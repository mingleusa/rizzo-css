const copyableHtml = `<!-- Tabler-style: stroke="currentColor" + .icon class (Rizzo CSS) = theme-friendly on light/dark -->
<span class="icon" aria-hidden="true">
  <svg width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" aria-hidden="true">
    <circle cx="12" cy="12" r="3"/>
    <path d="M12 1v2M12 21v2M4.22 4.22l1.42 1.42M18.36 18.36l1.42 1.42M1 12h2M21 12h2M4.22 19.78l1.42-1.42M18.36 5.64l1.42-1.42"/>
  </svg>
</span>

<!-- Inline with button (inherits button color) -->
<button type="button" class="btn btn-primary">
  <svg width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" aria-hidden="true"><path d="M5 12h14M12 5l7 7-7 7"/></svg>
  <span>Next</span>
</button>

<!-- Or use BEM (e.g. dropdown__icon, navbar__settings-icon) which also use var(--icon) -->
<span class="dropdown__icon" aria-hidden="true">
  <svg width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">...</svg>
</span>`;
---

<DocsLayout title="Icons  Vanilla" description="Icon usage with vanilla HTML. Same icon set as Astro; use .icon or BEM for theme-friendly colors.">
  <section>
    <h2>Icons</h2>
    <p>Reusable SVG icons (Tabler Icons and Devicons). Use <code>stroke="currentColor"</code> on the SVG and the <code>icon</code> class (or BEM like <code>dropdown__icon</code>) so Rizzo CSS sets <code>color: var(--icon)</code>icons then match Astro/Svelte and stay visible on light and dark themes.</p>
    <AddComponentTabs componentName="Icons" />
    <h3>Regular Icons ({regularIcons.length})  Tabler Icons</h3>
    <p>These use <code>currentColor</code>; wrap in <code>&lt;span class="icon"&gt;</code> for theme color. Click any card to copy its SVG to the clipboard.</p>
    <div class="icons-grid">
      {regularIcons.map((icon) => {
        const IconComponent = icon.component;
        return (
          <span class="tooltip-host icon-card-tooltip-host" data-tooltip="Copy SVG code" data-icon-name={icon.name}>
            <Card variant="outlined" class="icon-card icon-card--regular">
              <div class="icon-card__preview">
                <IconComponent width={32} height={32} class="icon-card__icon" />
              </div>
              <div class="icon-card__content">
                <h4 class="icon-card__name">{icon.name}</h4>
                <p class="icon-card__hint">Click to copy SVG</p>
              </div>
              <span class="sr-only" data-icon-svg-text={icon.svg}>{icon.svg}</span>
            </Card>
          </span>
        );
      })}
    </div>

    <h3>Devicons ({devicons.length})  Colored Brand Icons</h3>
    <p>Brand colors; visible on all themes. Click any card to copy its SVG to the clipboard.</p>
    <div class="icons-grid">
      {devicons.map((icon) => {
        const IconComponent = icon.component;
        return (
          <span class="tooltip-host icon-card-tooltip-host" data-tooltip="Copy SVG code" data-icon-name={icon.name}>
            <Card variant="outlined" class="icon-card icon-card--devicon">
              <div class="icon-card__preview icon-card__preview--devicon">
                <IconComponent width={32} height={32} class="icon-card__icon icon-card__icon--devicon" />
              </div>
              <div class="icon-card__content">
                <h4 class="icon-card__name">{icon.name}</h4>
                <p class="icon-card__hint">Click to copy SVG</p>
              </div>
              <span class="sr-only" data-icon-svg-text={icon.svg}>{icon.svg}</span>
            </Card>
          </span>
        );
      })}
    </div>

    <h3>HTML (examples)</h3>
    <p>Copy the patterns below. Ensure Rizzo CSS is loaded.</p>
    <CodeBlock code={copyableHtml} language="html" />

    <p><strong>Astro / Svelte:</strong> <a href="/docs/components/icons">Astro</a>  <a href="/docs/svelte/components/icons">Svelte</a></p>
    <p><a href="/docs/vanilla/components"> Back to Vanilla components</a></p>
  </section>
</DocsLayout>

<script is:inline>
	(function initVanillaIconCopy() {
		const init = () => {
			const iconCards = document.querySelectorAll('.icon-card');
			iconCards.forEach((card) => {
				if (card.hasAttribute('data-copy-initialized')) return;
				card.setAttribute('data-copy-initialized', 'true');
				const host = card.closest('.tooltip-host');
				const iconName = (host && host.getAttribute('data-icon-name')) || card.getAttribute('data-icon-name') || 'Icon';
				const svgEl = card.querySelector('[data-icon-svg-text]');
				if (!svgEl) return;
				const svgText = svgEl.textContent || svgEl.innerText || '';
				if (!svgText) return;
				const defaultTooltip = 'Copy SVG code';
				const copyToClipboard = async () => {
					try {
						await navigator.clipboard.writeText(svgText);
						const hint = card.querySelector('.icon-card__hint');
						if (hint) { hint.textContent = 'Copied!'; hint.classList.add('icon-card__hint--copied'); }
						if (host) host.setAttribute('data-tooltip', 'Copied!');
						card.setAttribute('aria-label', 'Copied!');
						setTimeout(() => {
							if (hint) { hint.textContent = 'Click to copy SVG'; hint.classList.remove('icon-card__hint--copied'); }
							if (host) host.setAttribute('data-tooltip', defaultTooltip);
							card.setAttribute('aria-label', 'Copy ' + iconName + ' SVG code');
						}, 2000);
					} catch (_) {
						const ta = document.createElement('textarea');
						ta.value = svgText;
						ta.style.position = 'fixed';
						ta.style.left = '-999999px';
						document.body.appendChild(ta);
						ta.focus();
						ta.select();
						try {
							document.execCommand('copy');
							const hint = card.querySelector('.icon-card__hint');
							if (hint) { hint.textContent = 'Copied!'; hint.classList.add('icon-card__hint--copied'); }
							if (host) host.setAttribute('data-tooltip', 'Copied!');
							card.setAttribute('aria-label', 'Copied!');
							setTimeout(() => {
								if (hint) { hint.textContent = 'Click to copy SVG'; hint.classList.remove('icon-card__hint--copied'); }
								if (host) host.setAttribute('data-tooltip', defaultTooltip);
								card.setAttribute('aria-label', 'Copy ' + iconName + ' SVG code');
							}, 2000);
						} catch (__) {}
						document.body.removeChild(ta);
					}
				};
				card.setAttribute('tabindex', '0');
				card.setAttribute('role', 'button');
				card.setAttribute('aria-label', 'Copy ' + iconName + ' SVG code');
				card.addEventListener('click', copyToClipboard);
				card.addEventListener('keydown', (e) => {
					if (e.key === 'Enter' || e.key === ' ') { e.preventDefault(); copyToClipboard(); }
				});
			});
		};
		if (document.readyState === 'loading') document.addEventListener('DOMContentLoaded', init);
		else init();
		document.addEventListener('astro:page-load', () => setTimeout(init, 0));
	})();
</script>

